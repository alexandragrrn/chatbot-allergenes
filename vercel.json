{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "public/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/rechercher", "dest": "app.js" },
    { "src": "/style.css", "dest": "/public/style.css" },
    { "src": "/(.*)", "dest": "public/index.html" }
  ],
  "headers": [
    {
      "source": "/style.css",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}
